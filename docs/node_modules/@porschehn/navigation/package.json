{
  "name": "@porschehn/navigation",
  "version": "1.2.0",
  "scope": "@porschehn",
  "description": "Porsche Home and Navigation -  Navigation",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/porsche-navigation/porsche-navigation.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/porscheui/porsche-home-and-nav.git"
  },
  "scripts": {
    "build": "stencil build --docs",
    "build.content": "cp -R ./src/content.json ./dist/porsche-navigation/content",
    "start": "ENVIRONMENT=local stencil build --dev --watch --serve",
    "test": "stencil test --spec --runInBand",
    "test.watch": "stencil test --spec --watchAll",
    "test.watch.all": "stencil test --spec --e2e --watchAll",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint.fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "coverage": "stencil test --spec --coverage",
    "clean": "rm -rf ./node_modules & rm -rf ./.stencil & rm -rf ./dist & rm -rf ./loader & rm -rf ./www",
    "test.e2e": "ENVIRONMENT=local stencil test --e2e",
    "precommit.routine": "yarn lint && npm test && npm run test.e2e",
    "generate": "stencil generate"
  },
  "devDependencies": {
    "@porsche-design-system/components-js": "1.3.0",
    "@porsche-design-system/scss-utils": "^1.0.0",
    "@stencil/core": "1.12.3",
    "@stencil/eslint-plugin": "^0.3.1",
    "@stencil/sass": "^1.1.1",
    "@stencil/store": "^1.3.0",
    "@types/jest": "24.9.1",
    "@types/puppeteer": "1.20.4",
    "@typescript-eslint/eslint-plugin": "^2.31.0",
    "@typescript-eslint/parser": "^2.31.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "fetch-mock": "^9.4.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0",
    "node-fetch": "^2.6.0",
    "picturefill": "^3.0.3",
    "prettier": "^2.0.5",
    "puppeteer": "1.20.0"
  },
  "dependencies": {
    "@openid/appauth": "^1.2.6",
    "@rollup/plugin-replace": "^2.2.1",
    "@stencil/state-tunnel": "^1.0.1",
    "gsap": "^3.2.6"
  },
  "publishConfig": {
    "registry": "https://porscheui.jfrog.io/porscheui/api/npm/npm-local/"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  }
}
