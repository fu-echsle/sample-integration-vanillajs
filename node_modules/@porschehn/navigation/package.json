{
  "name": "@porschehn/navigation",
  "version": "1.4.2",
  "scope": "@porschehn",
  "description": "Porsche Home and Navigation -  Navigation",
  "license": "UNLICENSED",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/porsche-navigation/porsche-navigation.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/porscheui/porsche-home-and-nav.git"
  },
  "scripts": {
    "build": "stencil build --docs",
    "start.content": "ENVIRONMENT=local stencil build --dev --watch --serve & cp -R ./../content-generator/model-assets ./dist/porsche-navigation/",
    "prestart": "echo copying content asssets && cp -R ./../content-generator/model-assets ./src/components/car-model/ && cp -R ./../content-generator/series-assets ./src/components/car-series/",
    "start": "ENVIRONMENT=local stencil build --dev --watch --serve",
    "test": "stencil test --spec --runInBand",
    "test.watch": "stencil test --spec --watchAll",
    "test.watch.all": "stencil test --spec --e2e --watchAll",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "lint.fix": "eslint 'src/**/*.{ts,tsx}' --fix",
    "coverage": "stencil test --spec --coverage",
    "clean": "rm -rf ./node_modules & rm -rf ./.stencil & rm -rf ./dist & rm -rf ./loader & rm -rf ./www",
    "test.e2e": "ENVIRONMENT=local stencil test --e2e",
    "precommit.routine": "yarn lint && npm test && npm run test.e2e",
    "generate": "stencil generate",
    "license.explain": "echo 'This command fails on blacklisted licenses. It outputs undhandled licenses and ommits reviewed and whitelisted licenses'",
    "license.check": "yarn license.explain && license-checker --excludePrivatePackages --failOn 'AGPL' --exclude 'MIT, ISC, Apache-2.0, 0BSD, BSD-2-Clause, BSD-3-Clause, CC0-1.0, WTFPL' --excludePackages '@porsche-design-system/components-js@1.3.0;@porsche-design-system/scss-utils@1.0.1;@porschehn/navigation@1.3.0;spdx-exceptions@2.2.0;spdx-ranges@2.1.1;tweetnacl@0.14.5'"
  },
  "devDependencies": {
    "@stencil/eslint-plugin": "^0.3.1",
    "@types/jest": "24.9.1",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "fetch-mock-jest": "^1.3.0",
    "jest": "24.9.0",
    "jest-cli": "24.9.0"
  },
  "dependencies": {
    "@porsche-design-system/components-js": "1.3.0",
    "@porsche-design-system/scss-utils": "^1.0.0",
    "@stencil/core": "1.14.0",
    "@stencil/sass": "^1.1.1",
    "@stencil/store": "^1.3.0",
    "@types/puppeteer": "1.20.4",
    "@types/uuid": "^8.0.0",
    "@openid/appauth": "^1.2.6",
    "@rollup/plugin-replace": "^2.2.1",
    "gsap": "^3.2.6",
    "license-checker": "^25.0.1",
    "node-fetch": "^2.6.0",
    "picturefill": "^3.0.3",
    "prettier": "^2.0.5",
    "puppeteer": "1.20.0",
    "uuid": "^8.1.0"
  },
  "publishConfig": {
    "registry": "https://porscheui.jfrog.io/porscheui/api/npm/npm-local/"
  },
  "jest": {
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": -10
      }
    }
  }
}
